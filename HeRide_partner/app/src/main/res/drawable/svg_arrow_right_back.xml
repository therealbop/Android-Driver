<?xml version="1.0" encoding="utf-8"?>

<layer-list android:paddingLeft="-10dp" android:paddingRight="-10dp"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="rectangle">
            <solid android:color="@color/white" android:splitTrack="false"/>
            <corners
                android:radius="25dp" />
            <size
                android:width="10dp"
                android:height="10dp"/>

        </shape>
    </item>

    <item android:drawable="@drawable/svg_arrow_right"
          android:left="25dp"
          android:top="10dp"
          android:right="25dp"
          android:bottom="10dp"/>

</layer-list>