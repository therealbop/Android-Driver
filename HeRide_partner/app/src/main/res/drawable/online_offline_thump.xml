    <layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

        <!-- Drop Shadow Stack -->
        <item>
            <shape
                android:shape="rectangle"
                android:visible="true"
                android:dither="true"
                android:useLevel="false">

                <padding
